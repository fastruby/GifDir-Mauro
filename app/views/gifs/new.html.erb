<h1>Add a new Gif</h1>
<% if @gif.errors.full_messages.any? %>
  <% @gif.errors.full_messages.each do |error_message| %>
    <%= error_message %> <br />
  <% end %>
<% end %>

<%= form_for :gif, url: gifs_path do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.file_field :img %>
  <%= f.submit %>
<% end %>
